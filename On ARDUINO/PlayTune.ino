void loop() {
  int xvar = 0;
  for (; xvar < notecount; xvar++) {
    if (pgm_read_word_near(&timedelay[xvar]) == 0) {
      delayMicroseconds(25);
    }
    else {
      int td = pgm_read_word_near(&timedelay[xvar]) / 10;
      delay(td);
    }
    for (unsigned short i = 0; i < pgm_read_word_near(&notenum[xvar]); i++) {
      digitalWrite(12, HIGH);
      delayMicroseconds(pgm_read_word_near(&notes[xvar]));
      digitalWrite(12, LOW);
      delayMicroseconds(pgm_read_word_near(&notes[xvar]));
    }
  }
  xvar = 0;
  while (1);
}

